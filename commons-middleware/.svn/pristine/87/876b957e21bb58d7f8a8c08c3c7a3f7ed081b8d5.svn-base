package com.heyi.scheduler;

/**
 * 
 * @author sulta
 *
 */
public interface ITimedTaskKey {
	/**
	 * 任务所属系统的名称，必填
	 * @return
	 */
	String getFdSystemName();
	
	/**
	 * 任务所属分类，必填
	 * @return
	 */
	String getFdTaskCategory();
	
	/**
	 * 任务key，必填
	 * @return
	 */
	String getFdTaskKey();
	
	/**
	 * 任务名称
	 * @return
	 */
	String getFdName();
	
	/**
	 * 自定义任务频道,每个应用用不同的任务频道，达到互不干扰的目的，如果不填则用公共频道。
	 * @return
	 */
	String getFdTaskChannel();
}
