package me.ywork.message;

import me.ywork.message.base.AbstractDingMessage;

/**
 * link消息
 * @author TangGang  2015年8月3日
 *
 */
public class DingLinkMessage extends AbstractDingMessage {
	private static final long serialVersionUID = -6828976013849016244L;

	/**
	 * 必填，消息点击链接地址
	 */
	private String messageUrl;
	/**
	 * 必填，图片媒体文件id，可以调用上传媒体文件接口获取
	 */
	private String picUrl;
	/**
	 * 必填，消息标题
	 */
	private String title;
	/**
	 * 必填，消息描述
	 */
	private String text;
	
	public DingLinkMessage() {
		super();
	}

	public String getMessageUrl() {
		return messageUrl;
	}

	public void setMessageUrl(String messageUrl) {
		this.messageUrl = messageUrl;
	}

	public String getPicUrl() {
		return picUrl;
	}

	public void setPicUrl(String picUrl) {
		this.picUrl = picUrl;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getText() {
		return text;
	}

	public void setText(String text) {
		this.text = text;
	}
	
	
}
